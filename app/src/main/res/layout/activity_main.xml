<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#EBEFF0"

    tools:context=".MainActivity">


        <Button
            android:id="@+id/btncalendar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="145dp"
            android:layout_marginTop="81dp"
            android:layout_marginEnd="125dp"
            android:background="#EAEEE7"
            android:text="@string/elegirfecha"
            android:textColor="@color/colorPrimary">
        </Button>


    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"


        android:text="@string/fecha"
        android:textColor="#0F2FE7"
        android:textSize="25dp">
    </TextView>
    <TextView
        android:id="@+id/tex_fecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#3CDD0F"
        android:textSize="25dp">
    </TextView>

    <TextView
        android:id="@+id/txt_edad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/resul"
        android:textColor="#0F2EDD"
        android:textSize="25dp">

    </TextView>
</LinearLayout>
